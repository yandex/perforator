{
  "name": "perforator",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "dev": "tsc --watch & vite build --watch & vite",
    "lint": "eslint . --ext js,ts,tsx --quiet",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:fix": "eslint --fix . --ext js,ts,tsx",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "release:preview": "ya tool aws s3 sync dist s3://preview-perforator-ui/$PULL_REQUEST_ID --delete",
    "release:testing": "ya tool nots install && ya tool nots run build && ya tool nots run release:s3:testing",
    "release": "ya tool nots install && ya tool nots run build && ya tool nots run release:s3",
    "release:s3": "ya tool aws s3 sync dist s3://perforator-ui --delete",
    "release:s3:testing": "ya tool aws s3 sync dist s3://testing-perforator-ui --delete",
    "test": "jest"
  },
  "dependencies": {
    "@bem-react/classname": "1.6.0",
    "@gravity-ui/components": "3.12.4",
    "@gravity-ui/date-components": "2.11.0",
    "@gravity-ui/date-utils": "2.5.5",
    "@gravity-ui/illustrations": "1.0.0",
    "@gravity-ui/navigation": "^2.22.0",
    "@gravity-ui/uikit": "6.34.0",
    "axios": "1.6.7",
    "culori": "4.0.1",
    "js-cookie": "3.0.5",
    "monaco-editor": "0.52.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-monaco-editor": "0.56.2",
    "react-router-dom": "6.22.3"
  },
  "devDependencies": {
    "@gravity-ui/eslint-config": "3.1.1",
    "@gravity-ui/icons": "2.9.1",
    "@gravity-ui/stylelint-config": "^4.0.1",
    "@jest/globals": "29.7.0",
    "@swc/core": "1.7.21",
    "@swc/jest": "0.2.36",
    "@types/culori": "2.1.1",
    "@types/js-cookie": "3.0.6",
    "@types/node": "18.18.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "eslint": "8.54.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jest": "29.7.0",
    "postcss": "^8.4.41",
    "prettier": "3.2.5",
    "sass": "1.71.1",
    "stylelint": "15.11.0",
    "ts-proto": "2.2.0",
    "typescript": "5.3.3",
    "vite": "5.0.2"
  },
  "pnpm": {
    "overrides": {
      "vite@>=5.0.0 <5.0.5": ">=5.0.5",
      "vite@>=5.0.0 <=5.0.11": ">=5.0.12"
    }
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
